<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy Relative Path">
        <Item>
          <IdeaActionID>CopyReference</IdeaActionID>
          <ShortCut>'⌥⇧⌘C'</ShortCut>
          <Description>Copy Relative Path</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration or Usages">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B'</ShortCut>
          <Description>Declaration or Usages</Description>
          <Count>55</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'⌥F7'</ShortCut>
          <Description>Find Usages</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⇧⌘F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Move...">
        <Item>
          <IdeaActionID>Move</IdeaActionID>
          <ShortCut>'F6'</ShortCut>
          <Description>Move...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="New...">
        <Item>
          <IdeaActionID>NewElement</IdeaActionID>
          <ShortCut>'⌘N' or '⌃⏎'</ShortCut>
          <Description>New...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Refactor This...">
        <Item>
          <IdeaActionID>Refactorings.QuickListPopupAction</IdeaActionID>
          <ShortCut>'⌃T'</ShortCut>
          <Description>Refactor This...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Refresh">
        <Item>
          <IdeaActionID>Vcs.Log.Refresh</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Refresh</Description>
          <Count>14</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'⇧F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Safe Delete...">
        <Item>
          <IdeaActionID>SafeDelete</IdeaActionID>
          <ShortCut>'⌘⌦'</ShortCut>
          <Description>Safe Delete...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show All Affected Files">
        <Item>
          <IdeaActionID>VcsHistory.ShowAllAffected</IdeaActionID>
          <ShortCut>'⌃⌘A'</ShortCut>
          <Description>Show All Affected Files</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
      <Statistic Action="Upload to...">
        <Item>
          <IdeaActionID>PublishGroup.UploadTo</IdeaActionID>
          <ShortCut>'⌥⇧⌘X'</ShortCut>
          <Description>Upload to...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'⌘9' or '⇧⌘9'</ShortCut>
          <Description>Version Control</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B'</ShortCut>
          <Description>Declaration</Description>
          <Count>14</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>